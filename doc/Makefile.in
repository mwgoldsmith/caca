@BUILD_DOCUMENTATION_TRUE@	$(mkinstalldirs) $(DESTDIR)$(mandir)/man3
@BUILD_DOCUMENTATION_TRUE@	for man in $$(find man -name '*.3caca'); do \
@BUILD_DOCUMENTATION_TRUE@	  sed -e 's/man3caca/man3/g' -e "s/fC'\([a-zA-Z0-9]*\)'/fC\"\1\"/g" \
@BUILD_DOCUMENTATION_TRUE@	    $$man >$(DESTDIR)$(mandir)/man3/$${man##*/}; \
@BUILD_DOCUMENTATION_TRUE@	done
	$(mkinstalldirs) $(DESTDIR)$(mandir)/man1
	rm -f $(DESTDIR)$(mandir)/man1/cacademo.1
	ln -sf cacafire.1 $(DESTDIR)$(mandir)/man1/cacademo.1

uninstall-local:
	rm -f $(DESTDIR)$(datadir)/doc/libcucul-dev
	rmdir $(DESTDIR)$(datadir)/doc 2>/dev/null || true

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
